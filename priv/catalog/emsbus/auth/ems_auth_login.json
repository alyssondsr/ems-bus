[
	{
		"name": "/login/assets",
		"comment": "Bibliotecas node_modules do portal",
		"owner": "auth",
		"version": "1.0.0",
		"service" : "ems_static_file_service:execute",
		"url": "/login/assets/",
		"use_re" : true,
		"path" : "{{ login_path }}",
		"type": "GET",
		"authorization" : "public",
		"cache_control" : "max-age=86400, public",
		"expires_minute" : 1440,
		"lang" : "erlang"
	},

	{
		"name": "redirect_url_login",
		"comment": "Redirect url /login to /login/index.html",
		"owner": "auth",
		"version": "1.0.0",
		"service" : "ems_redirect_url_service:execute",
		"url": "/login",
		"redirect_url" : "/login/index.html",
		"path" : "{{ login_path }}",
		"type": "GET",
		"authorization" : "public",
		"cache_control" : "no-cache",
		"expires_minute" : 0,
		"lang" : "erlang"
	},

	{
		"name": "/login_page",
		"comment": "Pagina de login ErlangMS",
		"owner": "auth",
		"version": "1.0.0",
		"service" : "ems_static_file_service:execute",
		"url": "/login/",
		"use_re" : true,
		"path" : "{{ login_path }}",
		"type": "GET",
		"authorization" : "public",
		"cache_control" : "no-cache",
		"expires_minute" : 0,
		"lang" : "erlang"
	}
]
